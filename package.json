{
    "name":  "istani-fitness",
    "version":  "0.1.0",
    "private":  true,
    "scripts":  {
                    "dev":  "next dev",
                    "dev:turbo":  "next dev --turbo",
                    "dev:watch":  "next dev --turbo",
                    "dev:all":  "concurrently \"npm run dev:turbo\" \"npm run typecheck:watch\"",
                    "build":  "next build",
                    "build:analyze":  "ANALYZE=true next build",
                    "start":  "next start",
                    "lint":  "next lint",
                    "lint:fix":  "next lint --fix",
                    "typecheck":  "tsc --noEmit",
                    "typecheck:watch":  "tsc --noEmit --watch --preserveWatchOutput",
                    "typecheck:incremental":  "tsc --noEmit --incremental",
                    "aggregate":  "node scripts/aggregateRepos.js",
                    "worktree:setup":  "node scripts/run-script.js setup-cursor-worktree",
                    "worktree:list":  "node scripts/run-script.js manage-worktrees list",
                    "worktree:create":  "node scripts/run-script.js manage-worktrees create",
                    "worktree:remove":  "node scripts/run-script.js manage-worktrees remove",
                    "worktree:prune":  "node scripts/run-script.js manage-worktrees prune",
                    "worktree:neon":  "node scripts/run-script.js setup-neon-comments-worktree",
                    "worktree:fullstack":  "node scripts/run-script.js setup-full-stack-worktree",
                    "clean:logs":  "node scripts/run-script.js clean-amazon-q-logs",
                    "docker:build":  "node scripts/run-script.js docker-build production",
                    "docker:build:dev":  "node scripts/run-script.js docker-build dev",
                    "docker:run":  "node scripts/run-script.js docker-run production",
                    "docker:run:dev":  "node scripts/run-script.js docker-run dev",
                    "docker:up":  "node scripts/run-script.js docker-up",
                    "docker:stop":  "docker-compose down",
                    "docker:clean":  "docker system prune -f && docker volume prune -f",
                    "agents:dev":  "node scripts/run-script.js agents-dev",
                    "logs:clear":  "node scripts/run-script.js logs-clear",
                    "logs:archive":  "node scripts/run-script.js logs-archive",
                    "review":  "bash scripts/coderabbit-review.sh",
                    "review:plain":  "export PATH=\"$HOME/.local/bin:$PATH\" \u0026\u0026 coderabbit --plain -t uncommitted",
                    "review:prompt":  "export PATH=\"$HOME/.local/bin:$PATH\" \u0026\u0026 coderabbit --prompt-only -t uncommitted"
                },
    "dependencies":  {
                         "@octokit/rest":  "^20.0.2",
                         "@stripe/stripe-js":  "^4.10.0",
                         "@supabase/ssr":  "^0.5.2",
                         "@supabase/supabase-js":  "^2.45.6",
                         "@tailwindcss/forms":  "^0.5.9",
                         "clsx":  "^2.1.1",
                         "lucide-react":  "^0.553.0",
                         "next":  "^15.1.2",
                         "react":  "^18.3.1",
                         "react-dom":  "^18.3.1",
                         "react-hook-form":  "^7.53.2",
                         "stripe":  "^17.3.1",
                         "tailwind-merge":  "^2.5.5",
                         "zod":  "^3.23.8",
                         "zustand":  "^5.0.1"
                     },
    "devDependencies":  {
                            "@next/bundle-analyzer":  "^15.1.2",
                            "@types/node":  "^20",
                            "@types/react":  "^18",
                            "@types/react-dom":  "^18",
                            "autoprefixer":  "^10.4.22",
                            "concurrently":  "^9.1.0",
                            "eslint":  "^8",
                            "eslint-config-next":  "^15.1.2",
                            "postcss":  "^8",
                            "tailwindcss":  "^3.4.1",
                            "typescript":  "^5"
                        }
}
