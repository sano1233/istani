{
  "name": "istani-fitness",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:istani-rebuild": "node scripts/build-istani-rebuild.mjs",
    "test": "echo \"No tests specified\" && exit 0",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.mjs,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,mjs,ts,tsx,json,css,md,html,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,mjs,ts,tsx,json,css,md,html,yml,yaml}\"",
    "typecheck": "tsc --noEmit || echo 'No TypeScript config'",
    "precommit": "npm run format && npm run lint:fix && npm test",
    "ci": "npm run format:check && npm run lint && npm run typecheck && npm test && npm run build",
    "generate:links": "node scripts/buildLinksPage.js",
    "post": "node scripts/postToSocials.js",
    "agent:install": "cd ai-agent && npm install",
    "agent:start": "cd ai-agent && npm start",
    "agent:dev": "cd ai-agent && npm run dev",
    "agent:cli": "cd ai-agent && npm run cli",
    "agent:process": "cd ai-agent && npm run cli process",
    "agent:process-all": "cd ai-agent && npm run cli process-all",
    "agent:stats": "cd ai-agent && npm run cli stats",
    "agent:docker:build": "cd ai-agent && npm run docker:build",
    "agent:docker:run": "cd ai-agent && npm run docker:run"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.45.6",
    "@stripe/stripe-js": "^4.10.0",
    "@tailwindcss/forms": "^0.5.9",
    "clsx": "^2.1.1",
    "next": "15.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "stripe": "^17.3.1",
    "tailwind-merge": "^2.5.5",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "esbuild": "^0.25.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.0.3",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-security": "^3.0.1",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
